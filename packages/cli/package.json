{
  "name": "@minicc/cli",
  "version": "0.2.1",
  "description": "MiniCC CLI - Command line interface for MiniCC",
  "keywords": [
    "cli",
    "ai",
    "agent",
    "programming-assistant",
    "claude-code",
    "minicc"
  ],
  "author": "",
  "license": "MIT",
  "bin": {
    "minicc": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
