    "@minicc/sdk": "workspace:*",
    "@types/inquirer": "^9.0.9",
    "boxen": "^8.0.1",
    "chalk": "^5.6.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^12.9.4",
    "ora": "^8.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/uuid": "^10.0.0"
  }
}
